# set this variable to point to this directory..
export AHIR_RELEASE=$(pwd)
# path
export PATH=$AHIR_RELEASE/bin:$PATH
# ld-library-paths
export LD_LIBRARY_PATH=$AHIR_RELEASE/lib:$LD_LIBRARY_PATH
export LD_LIBRARY_PATH=$AHIR_RELEASE/functionLibrary/lib:$LD_LIBRARY_PATH
# /usr/local/lib...
export LD_LIBRARY_PATH=/usr/local/lib:$LD_LIBRARY_PATH
